<xs:schema version="1.0" targetNamespace="http://genologics.com/ri/step" xmlns:stp="http://genologics.com/ri/step" xmlns:ri="http://genologics.com/ri" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://genologics.com/ri" schemaLocation="ri.xsd"/>
  <xs:element name="step" type="stp:step"/>
  <xs:element name="actions" type="stp:actions"/>
  <xs:element name="placements" type="stp:placements"/>
  <xs:element name="pools" type="stp:pools"/>
  <xs:element name="program-status" type="stp:program-status"/>
  <xs:element name="reagents" type="stp:reagents"/>
  <xs:complexType name="step">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[The detailed representation of a step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The configuration information for the step run.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="actions" type="stp:actions-link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The corresponding step actions.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reagents" type="stp:reagents-link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The corresponding step reagents.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pools" type="stp:pools-link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The corresponding step pools.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="placements" type="stp:placements-link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The corresponding step placements.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="program-status" type="stp:program-status-link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The corresponding step program status, if one exists.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI"/>
    <xs:attribute name="limsid" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The LIMS ID of the Step/Process.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="step-configuration">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the configuration information for the step run.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="uri" type="xs:anyURI">
          <xs:annotation>
            <xs:documentation>
              <![CDATA[The URI of the protocol step.]]>
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="actions-link">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the resource that represents the actions applied or to be applied by the step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of corresponding step actions resource.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="placements-link">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the resource that represents the output placements for the step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of corresponding step placements resource.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="pools-link">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the resource that represents the pools added by the step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of corresponding step pools resource.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="program-status-link">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the resource that represents the program status for the step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of corresponding step program status resource.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="reagents-link">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Identifies the resource that represents the reagents added by the step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of corresponding step reagents resource.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="actions">
    <xs:sequence>
      <xs:element name="step" type="ri:link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The step (process).
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step configuration.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="next-actions" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[All samples belong to one step; these samples can move forward for another configured step work
or need special handling, such as remove from the workflow they are in, leave in their existing
queue for rework, or require view by a manager, etc...]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="next-action" type="stp:next-action" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[All samples belong to one step; these samples can move forward for another configured step work
or need special handling, such as remove from the workflow they are in, leave in their existing
queue for rework, or require view by a manager, etc...]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI"/>
  </xs:complexType>
  <xs:complexType name="next-action">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Sample next action or step.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="artifact-uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the artifact.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: no
<br/>Required for PUT: yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="action" type="stp:action-type">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The next action for the sample.
<br/>Always returns with GET: no; the sample may not have a next action.
<br/>Updatable with PUT: yes
<br/>Required for PUT. If the action is not present during an update, the existing action for the sample
will be removed if there is one.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="step-uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The next protocol step for the sample.
<br/>Always returns with GET: no; the sample may not have a next step.
<br/>Updatable with PUT: yes
<br/>Required for PUT. If the step is not present during an update, the existing step for the sample
will be removed if there is one.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="action-type">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Special actions for samples.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="leave"/>
      <xs:enumeration value="repeat"/>
      <xs:enumeration value="remove"/>
      <xs:enumeration value="review"/>
      <xs:enumeration value="complete"/>
      <xs:enumeration value="store"/>
      <xs:enumeration value="nextstep"/>
      <xs:enumeration value="rework"/>
      <xs:enumeration value="unknown"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="placements">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[The detailed representation of a step's output artifact container placements]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="step" type="ri:link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step of the StepPlacements.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step configuration.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="selected-containers" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The selected containers for step placement.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: Yes
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="container" type="stp:container" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[The selected containers for step placement.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: Yes
<br/>Required for POST: No]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="output-placements" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The output artifacts for this step with placement if they have one.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: Yes
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="output-placement" type="stp:output-placement" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[The output artifacts for this step with placement if they have one.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: Yes
<br/>Required for POST: No]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the StepPlacements.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="container">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Provides a URI linking to a selected container to be used for placement.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the container.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: Yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="output-placement">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Provides a URI linking to the output artifact and container placement.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="location" type="ri:location" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The container placement for the artifact.
<br/>Always returns with GET: No; the artifact may not have a placement.
<br/>Creatable with POST: Yes
<br/>Required for POST: No; If specified, any existing placement will be replaced with the new placement. If not specified, any existing placement will be removed.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the artifact.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: Yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="pools">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[The detailed representation of a step's pooled inputs.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="step" type="ri:link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The step (process).
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step configuration.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pooled-inputs" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The pooled input artifacts for this step.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: Yes
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="pool" type="stp:pooled-inputs" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[The pooled input artifacts for this step.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: Yes
<br/>Required for PUT: No]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="available-inputs" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The available input artifacts for this step.
<br/>Always returns with GET: Yes]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="input" type="stp:input" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[The available input artifacts for this step.
<br/>Always returns with GET: Yes]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the StepPools.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="input">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Provides input URI links.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the artifact.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: Yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="pooled-inputs">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Provides pooled input groups.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="input" type="stp:input" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The pooled input artifacts for this pool.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: Yes
<br/>Required for PUT: Yes]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The pool name.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: Yes
<br/>Required for PUT: Yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="output-uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the pooled output artifact.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="program-status">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[The current EPP status for a step (supports automatically triggered programs only).]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="step" type="ri:link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The step (process).
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step configuration.
<br/>Always returns with GET: Yes
<br/>Updatable with PUT: No
<br/>Required for PUT: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="stp:status" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The nature of the status.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="message" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The user-facing message for this status.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI to this status.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="status">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Describes the different natures of status for an external program.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OK"/>
      <xs:enumeration value="ERROR"/>
      <xs:enumeration value="WARNING"/>
      <xs:enumeration value="RUNNING"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="reagents">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[The detailed representation of a step's output artifact reagents.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="step" type="ri:link" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The step (process).
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configuration" type="stp:step-configuration" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The protocol step configuration.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reagent-category" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The permitted reagent category of the step. Reagent labels used in the POST must be from this reagent category.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="output-reagents" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The output artifacts for this step.
<br/>Always returns with GET: Yes]]>
          </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="output" type="stp:output" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>
                  <![CDATA[The output artifacts for this step.
<br/>Always returns with GET: Yes]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the StepReagents.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: No]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="reagent-label">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Reagent-label is a child element of ExtStepReagentListElement and provides the name of a
label or reagent that has been added to the Artifact.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="name" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The reagent label name for the artifact.
<br/>Always returns with GET: No; the artifact may not have a reagent label.
<br/>Creatable with POST: Yes
<br/>Required for POST: No; If specified, an existing reagent label will be replaced with the new label. If not specified, existing labels will be removed.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="output">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Provides a URI linking to the output artifact and reagent label.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="reagent-label" type="stp:reagent-label" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The reagent labels for the artifact.
<br/>Always returns with GET: No; the artifact may not have reagent labels.
<br/>Creatable with POST: Yes
<br/>Required for POST: No; If specified, an existing reagent label will be replaced with the new label. If not specified, existing labels will be removed.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="uri" type="xs:anyURI">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The URI of the artifact.
<br/>Always returns with GET: Yes
<br/>Creatable with POST: No
<br/>Required for POST: Yes]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
</xs:schema>
